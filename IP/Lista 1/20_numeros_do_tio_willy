#include <stdio.h>

int main(){
    int N;
    scanf("%d", &N);
    int n[20];

    int  contador = 0;
    int cont = 0;
    do{
        scanf("%d", &n[cont]);
        cont++;
    } while(cont < 20 || n[cont] == -1);

    for(int i = 0; i < cont; i++){
        if(N == n[i]){
            contador++;
        }
    }
    printf("%d apareceu %d vezes", N, contador);
    return 0;
}